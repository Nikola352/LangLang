<mvvm:NavigableWindow x:Class="LangLang.View.DirectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LangLang.View"
        xmlns:mvvm="clr-namespace:LangLang.MVVM"
        mc:Ignorable="d"
        Title="DirectorWindow" Height="200" Width="200" ResizeMode="CanMinimize"
        WindowStartupLocation="CenterScreen">
    <StackPanel Orientation="Vertical"  Margin="0,30,0,0">
        <Button Content="View tutors" Margin="10" Height="40" FontSize="20" Command="{Binding OpenTutorTableCommand}"/>
        <Button Content="Logout" Margin="10" Height="40" FontSize="20" Command="{Binding LogoutCommand}"/>
    </StackPanel>
</mvvm:NavigableWindow>
